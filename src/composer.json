{
    "name": "Milanz247/ShoppingCart",
    "description": "A shopping cart package for Laravel using Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Milanz247",
            "email": "milansenarathna47@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "ext-json": "*",
        "milanz247/ShoppingCart": "dev-master"


    },
 
    "autoload": {
        "psr-4": {
