{
    "name": "milanz247/shopping-cart",
    "description": "A shopping cart package for Laravel using Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Milanz247",
            "email": "milansenarathna47@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "nunomaduro/collision": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Milanz247\\ShoppingCart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Milanz247\\ShoppingCart\\ShoppingCartServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/milanz247/shopping-cart"
        }
    ],
    "prefer-stable": true
}
